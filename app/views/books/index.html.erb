<div class="row">
    <div class="col-sm-12">
      <center>
        <h1><u>The Great Listby Books</u></h1>
        <%= form_tag(books_path, method: :get) do %>
          <%= text_field_tag :term, params[:term] %>
          <%= submit_tag 'Search By Title', name: nil, class: "btn btn-default btn-sm" %>
        <% end %>
      </center>
    </div>
</div>

<div class="row">
  <div class="col-sm-12">
    <div id="books_index">
    </div>
  </div>
</div>
